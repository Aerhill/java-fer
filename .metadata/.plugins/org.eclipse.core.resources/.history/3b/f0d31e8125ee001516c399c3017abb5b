package hr.fer.zemris.java.custom.collections;

public class ObjectStack {

    ArrayIndexedCollection adaptee;
    
    public ObjectStack() {
	
    }
    
    public boolean isEmpty() {
	return adaptee.isEmpty();
    }
    
    public int size() {
	return adaptee.size();
    }
    
    public void push(Object value) {
	adaptee.add(value);
    }
    
    public Object pop() {
	Object result = adaptee.get(adaptee.size());
	adaptee.remove(result);
	return result;
    }
    
    public Object peek() {
	return adaptee.get(adaptee.size());
    }
    
    public void clear() {
	adaptee.clear();
    }
    
}

package hr.fer.zemris.java.cstr;

public class Demo {

	public static void main(String[] args) {

		char[] a = {'A','B','C'};
		char[] b = a;
		
		for (int i = 0; i < a.length; i++) {
			b[i] = a[i];
			System.out.println(b[i]);
		}
		
		a[0] = '6';
		
		for (int i = 0; i < b.length; i++) {
			System.out.println(b[i]);
		}
	}

}

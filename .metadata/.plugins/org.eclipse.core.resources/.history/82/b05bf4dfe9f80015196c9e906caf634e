package hr.fer.zemris.java.cstr;

public class Demo {

	public static void main(String[] args) {

		char[] a = {'A','B','C'};
		char[] b = new char[3];
		
		for (int i = 0; i < a.length; i++) {
			b[i] = a[i];
			System.out.println(b[i]);
		}
		
		a[0] = '6';
		
		for (int i = 0; i < b.length; i++) {
			System.out.println(b[i]);
		}
		
		char[] data = null;
		String str = new String(data);
	}

}
